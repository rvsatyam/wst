  <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
      <div class="carousel-item active">
        <div class="all">
        <div class="col" >
          <div class="row look1">
            <h1>Work</h1>
            <p>
              <%= items[0].work %>
              </p>
          </div>
          <div class="row look2">
            <a href="/estimate">Give Proposol</a>
          </div>
        </div>
      </div>
      </div>
      <%  for(var i = 1; i < items.length; i++){ %>
        <div class="carousel-item">
          <div class="all">
            <div class="col">
              <div class="row look1">
                <h1>Work</h1>
                <p>
